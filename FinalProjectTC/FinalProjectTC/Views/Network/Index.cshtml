@{
}


@model List<Network>

<h1>CityBike Locations in the United States</h1>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>E-Bike Availability</th>
    </tr>

   @foreach (var item in @Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Location</td>
            <td>@item.Ebikes</td>
        </tr>
    }

</table>
