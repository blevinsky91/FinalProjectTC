@{
}


@model IEnumerable<Network>

<h2>Names and Locations in the United States</h2>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Website</th>
        <th>E-bikes available?</th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            <td>@item.name</td>
            <td>@item.location</td>
            <td>@item.gbfs_href</td>
            <td>@item.ebikes</td>
        </tr>
    }

</table>
